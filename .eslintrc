{
	"extends": [
		"eslint:recommended",
		"plugin:vue/recommended"
	],
	"env": {
		"node": true,
		"jest": true
	},
	"plugins": [
		"vue"
	],
	"rules": {
		"space-before-function-parent": 0,
		"quotes": [2, "single"],
		"indent": [2,"tab"],
		"no-tabs": "off",
		"vue/require-default-prop": "off",
		"object-curly-spacing": ["error", "always", {
			"arraysInObjects": false,
			"objectsInObjects": false
		}],
		"semi": ["error", "always"],
		"no-console": ["error", {
			"allow": ["warn", "error"],
		}]
	}
}
